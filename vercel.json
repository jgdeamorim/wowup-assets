{
  "rewrites": [
    { "source": "/worker/(.*)", "destination": "/public/worker/$1" }
  ],
  "headers": [
    {
      "source": "/worker/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "max-age=3600" }
      ]
    }
  ]
}
